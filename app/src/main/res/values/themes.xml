<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- O parent PRECISA ser Theme.AppCompat para funcionar com AppCompatActivity -->
    <style name="Theme.MedNES" parent="Theme.AppCompat.NoActionBar">
        <!-- Remove barra de título e action bar -->
        <item name="android:windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <!-- Força tela cheia -->
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
</resources>
